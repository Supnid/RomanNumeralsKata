package main;

public class ArabicToRomanConverter {

	public String convert(int input) {
		String romanNumeral = new String();
		if(input == 10){
			
		}else if(input == 5){
			romanNumeral += "V";
		}else if(input == 4){
			romanNumeral += "IV";
		}else{
			for(int i = 0; i < input; i++)
				romanNumeral += "I";
		}
		return romanNumeral;
	}

}
